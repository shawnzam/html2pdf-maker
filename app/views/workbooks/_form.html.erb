<%= form_for @workbook, :html => { :multipart => true } do |f| %>


  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :bookmark %><br />
   <%= f.file_field :bookmark %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
  
<% end %>
